{
    "name": "paypal/merchant-sdk-php",
    "description": "PayPal Merchant SDK for PHP",
    "keywords": ["paypal", "php", "sdk"],
    "homepage": "https://developer.paypal.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "PayPal",
            "homepage": "https://github.com/paypal/merchant-sdk-php/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*",
        "vanstokes/sdk-core-php":"3.*"
    },
    "repositories": [
     {
      "type": "package",
      "package": {
        "name": "vanstokes/sdk-core-php",
        "version": "3.0",
        "source": {
          "url": "https://github.com/vanstokes/sdk-core-php.git",
          "type": "git",
          "reference": "main"
        }
      }
    },
    "autoload": {
        "psr-0": {
            "PayPal\\Service": "lib/",
            "PayPal\\CoreComponentTypes": "lib/",
            "PayPal\\EBLBaseComponents": "lib/",
            "PayPal\\EnhancedDataTypes": "lib/",
            "PayPal\\PayPalAPI": "lib/"
        }
    }
}
